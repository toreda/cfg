		"@toreda/eslint-config": "^2.2.0",
		"@toreda/fate": "^0.7.0",
		"@toreda/prettier-config": "^1.0.1",
		"@toreda/types": "^2.16.0",
		"@types/eslint": "^9.6.1",
		"@types/gulp": "^4.0.9",
		"@types/jest": "^27.4.1",
		"@types/jsdom": "^21.1.7",
		"@types/node": "^22.15.20",
		"@types/webpack": "^5.28.0",
		"del": "^6.0.0",
		"eslint": "^9.27.0",
