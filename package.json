		"typescript": "^5.8.3",
		"webpack": "^5.72.0"
	},
	"resolutions": {
		"glob-parent": "^5.1.2"
	},
	"sideEffects": "false",
	"prettier": "@toreda/prettier-config",
	"keywords": [
		"JavaScript",
