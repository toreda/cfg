{
	"name": "@toreda/cfg",
	"version": "0.0.0",
	"publisher": "true",
	"description": "Manage user configurations and settings on web or Electron.",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"repository": "https://github.com/toreda/cfg",
	"author": "Toreda, Inc.",
	"license": "MIT",
	"scripts": {
		"build": "yarn build:prod",
		"build:prod": "yarn gulp --prod",
		"build:dev": "yarn gulp --dev",
		"test": "yarn jest --coverage --maxWorkers=50%",
		"test:fast": "yarn jest --coverage=false --maxWorkers=25% --verbose=false",
		"make:docs": "yarn typedoc --options ./typedoc.config.js --plugin @knodes/typedoc-plugin-code-blocks",
		"make:depgraph": "yarn madge src/index.ts --image dependency-graph.png"
	},
	"devDependencies": {
		"@swc/core": "^1.11.24",
